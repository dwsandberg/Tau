#!/bin/sh 
SCRIPT_DIR="$( cd -- "$( dirname -- "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )"

# set up working directory

cd $SCRIPT_DIR
source tauconfig.sh
cd ..
makelib $1.lib
err="$?"
if [ "$err" != "0" ]; then
$tauopen tmp/$1.lib.html
exit 1
else
output="output.html"

$SCRIPT_DIR/$tauDylib$@ > "$output"
#if [ "<" == "$(head -c 1  $output)" ] ;
#then 
#    $tauopen $output 
#else
#  cat $ouput
#fi

$tauopen "$output"
fi
