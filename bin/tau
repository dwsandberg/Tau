#!/bin/bash  
SCRIPT_DIR="$( cd -- "$( dirname -- "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )"

# set up working directory

cd $SCRIPT_DIR
source tauconfig.sh
cd ..
set -e
makelib $1.lib

output="output.html"

$SCRIPT_DIR/$tauDylib$@ > "$output"
#if [ "<" == "$(head -c 1  $output)" ] ;
#then 
#    $tauopen $output 
#else
#  cat $ouput
#fi

$tauopen "$output"

exit