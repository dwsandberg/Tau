#!/bin/sh
  
SCRIPT_DIR="$( cd -- "$( dirname -- "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )"

# set up working directory

cd $SCRIPT_DIR

cd ..

set -e
arg1=$1
lib=${arg1%.*}

source bin/tauconfig.sh

built/${tauDylib}webassembly $lib    > "tmp/$lib.html"
line1=$(awk '/^Successful compile/  {print "OK"}' tmp/$lib.html)
 if [ "$line1" == "OK" ]; then 
  echo "done" > built/$lib.wexe
else
 $tauopen tmp/$lib.html
fi
