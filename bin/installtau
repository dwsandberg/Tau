if [ $(uname) == "Linux" ]; then
echo  export tauopen=xdg-open  > bin/tauconfig.sh 
else 
echo '#export tauDylib="tauexe "' > bin/tauconfig.sh
echo export tauopen=open >> bin/tauconfig.sh
xattr -rd com.apple.quarantine bin/checkboot 
xattr -rd com.apple.quarantine bin/tau
fi
mkdir tmp
mkdir built
echo "" > B
echo "" >> B
echo "void init_stdlib(); void init_libs(){init_stdlib(); }">built/stdlib.c
clang -lm -pthread  stdlib/*.c built/stdlib.c bin/stdlib.bc  -o bin/stdlib
cp bin/stdlib.bc built/stdlib.bc
